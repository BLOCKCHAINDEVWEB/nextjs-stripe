{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "plugin:@next/next/recommended",
      "airbnb-base",
      "prettier"
    ],
    "parser": "@babel/eslint-parser",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "requireConfigFile": false
    },
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "no-console": "off"
    }
}
